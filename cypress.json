{
  "baseUrl": "https://dev.trade-tariff.service.gov.uk",
  "services": {
    "xi": "https://dev.trade-tariff.service.gov.uk/xi",
    "uk": "https://dev.trade-tariff.service.gov.uk/uk"
  },

  "reporter": "cypress-multi-reporters",
  "reporterOptions": {
    "reporterEnabled": "mochawesome",
    "mochawesomeReporterOptions": {
      "reportDir": "cypress/reports/mocha",
      "quite": true,
      "overwrite": false,
      "html": false,
      "json": true
    }

  },
  "chromeWebSecurity": false,
  "testFiles": [
    "**/*.feature",
    "**/*.spec.js"
  ]
}
